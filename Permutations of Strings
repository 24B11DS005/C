#include <string.h>

/* Swap two strings */
void swap(char **a, char **b)
{
    char *temp = *a;
    *a = *b;
    *b = temp;
}

/* Reverse strings from index start to end */
void reverse(char **s, int start, int end)
{
    while (start < end)
    {
        swap(&s[start], &s[end]);
        start++;
        end--;
    }
}

/* Generate next lexicographical permutation */
int next_permutation(int n, char **s)
{
    int i = n - 2;

    // Step 1: find largest i such that s[i] < s[i+1]
    while (i >= 0 && strcmp(s[i], s[i + 1]) >= 0)
        i--;

    if (i < 0)
        return 0;   // no next permutation

    // Step 2: find largest j such that s[j] > s[i]
    int j = n - 1;
    while (strcmp(s[j], s[i]) <= 0)
        j--;

    // Step 3: swap
    swap(&s[i], &s[j]);

    // Step 4: reverse suffix
    reverse(s, i + 1, n - 1);

    return 1;
}

int main()
{
	char **s;
	int n;
	scanf("%d", &n);
	s = calloc(n, sizeof(char*));
	for (int i = 0; i < n; i++)
	{
		s[i] = calloc(11, sizeof(char));
		scanf("%s", s[i]);
	}
	do
	{
		for (int i = 0; i < n; i++)
			printf("%s%c", s[i], i == n - 1 ? '\n' : ' ');
	} while (next_permutation(n, s));
	for (int i = 0; i < n; i++)
		free(s[i]);
	free(s);
	return 0;
}
